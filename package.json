{
	"name": "11tystart",
	"version": "1.0.0",
	"description": "",
	"main": ".eleventy.js",
	"scripts": {
		"watch:sass": "sass  --no-source-map --watch src/assets/scss:_site/assets/css",
		"watch:eleventy": "eleventy --serve",
		"build:sass": "sass  --no-source-map src/assets/scss:_site/assets/css",
		"build:eleventy": "eleventy",
		"build:purge": "purgecss -c ./purgecss.config.js -o _site/assets/css/main.css",
		"postbuild": "postcss _site/assets/css/*.css -r --no-map build:purge",
		"start": "npm-run-all build:sass --parallel watch:*",
		"build": "npm-run-all build:sass build:eleventy"
	},
	"keywords": [
		"eleventy",
		"starter-kit",
		"sass",
		"purgecss",
		"postcss",
		"autoprefixer"
	],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@11ty/eleventy": "^0.11.1",
		"@11ty/eleventy-navigation": "^0.1.6",
		"@11ty/eleventy-plugin-rss": "^1.1.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
		"autoprefixer": "^10.2.4",
		"cross-env": "^7.0.3",
		"cssnano": "^4.1.10",
		"luxon": "^1.26.0",
		"markdown-it": "^10.0.0",
		"markdown-it-anchor": "^7.0.2",
		"markdown-it-link-attributes": "^3.0.0",
		"npm-run-all": "^4.1.5",
		"nunjucks": "^3.2.3",
		"postcss": "^8.2.6",
		"postcss-cli": "^8.3.1",
		"purgecss": "^4.0.2",
		"sass": "^1.32.8"
	}
}
